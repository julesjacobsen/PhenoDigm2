<dataConfig>
    <dataSource name="phenodb" jndiName="java:comp/env/jdbc/phenodigmDataSource"/>
    <document>
        <!--
         Diseases
        -->
        <entity name="pheno_disease" query="select concat(disease_id, '_', mgi_gene_id) as id, disease_id, mgi_gene_id, mgi_gene_symbol, disease_term, disease_alts from disease_gene_summary;">
            <field column="id" name="id"/>
            <field column="disease_id" name="disease_id"/>
            <field column="disease_term" name="disease_term"/>
            <field column="disease_alts" name="disease_alts"/>
            <field column="mgi_gene_id" name="marker_accession"/>
            <field column="mgi_gene_symbol" name="marker_symbol"/>
        </entity>
        <!--
        Mouse models
       -->
       <entity name="pheno_model" query="select concat(disease_id, '_', mouse_model_id) as id, disease_id, mgi_gene_id, mgi_gene_symbol, mouse_model_id, source from disease_mouse_genotype_associations;">
         <field column="id" name="id"/>
         <field column="disease_id" name="disease_id"/>
         <field column="mgi_gene_id" name="marker_accession"/>
         <field column="mgi_gene_symbol" name="marker_symbol"/>
         <field column="mouse_model_id" name="mouse_model_id"/>
         <field column="source" name="source"/>
       </entity>
    </document>
</dataConfig>